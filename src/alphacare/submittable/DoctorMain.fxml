<AnchorPane id="AnchorPane" prefHeight="347.0" prefWidth="502.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="alphacare.submittable.PatientMainController">
   <children>
      <Label fx:id="doctorTitle" layoutX="174.0" layoutY="47.0" text="Welcome, Doctor!">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <GridPane layoutX="66.0" layoutY="70.0" prefHeight="208.0" prefWidth="370.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="medicalHistoryButton" mnemonicParsing="false" onAction="#medicalButtonAction" prefHeight="130.0" prefWidth="136.0" text="Write Recent Medical History" textAlignment="CENTER" wrapText="true" />
            <Button fx:id="viewInsuranceButton" mnemonicParsing="false" prefHeight="130.0" prefWidth="130.0" text="View Patient Insurance" textAlignment="CENTER" wrapText="true" GridPane.columnIndex="2" />
            <Button fx:id="prescriptionLogButton" mnemonicParsing="false" onAction="#prescriptionLogButton" prefHeight="130.0" prefWidth="130.0" text="Write Prescription" textAlignment="CENTER" wrapText="true" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>

