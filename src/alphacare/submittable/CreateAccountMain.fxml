<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="alphacare.submittable.LoginController">
    <children>
        <Button fx:id="createAccountButton" layoutX="172.0" layoutY="243.0" onAction="#createAccountButtonAction" text="Create Account" />
        <Text layoutX="56.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" />
        <TextField fx:id="usernameField" layoutX="139.0" layoutY="31.0" promptText="Username" />
        <Text layoutX="57.0" layoutY="149.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
        <PasswordField fx:id="passwordField" layoutX="139.0" layoutY="132.0" promptText="Password" />
        <Label fx:id="loginTitle" layoutX="21.0" layoutY="14.0" text="Create an Account" />
        <Text layoutX="56.0" layoutY="84.0" strokeType="OUTSIDE" strokeWidth="0.0" text="First Name:" />
        <TextField fx:id="firstNameField" layoutX="139.0" layoutY="67.0" promptText="First Name" />
        <Text layoutX="54.0" layoutY="184.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Last Name:" />
        <PasswordField fx:id="lastNameField" layoutX="139.0" layoutY="167.0" promptText="Last Name" />
        <Text layoutX="58.0" layoutY="117.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Birth Date:" />
        <TextField fx:id="birthDateField" layoutX="139.0" layoutY="100.0" promptText="Birth Date" />
        <Text layoutX="55.0" layoutY="217.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email:" />
        <PasswordField fx:id="emailField" layoutX="139.0" layoutY="200.0" promptText="Email" />
    </children>

</AnchorPane>
